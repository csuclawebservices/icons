{
	"name": "@csuclawebservices/icons",
	"version": "1.3.0",
	"description": "Icons to use in the WordPress Block Editor.",
	"keywords": [
		"wordpress",
		"icons"
	],
	"author": "CLA Web Services",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"dependencies": {
		"@wordpress/primitives": "^4.7.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"compile": "babel src --out-dir build",
		"build": "node build.js && npm run compile",
		"prepublishOnly": "npm run clean && npm run build && git push --tags",
		"clean": "rimraf ./src/library ./build"
	},
	"babel": {
		"presets": [
			"@wordpress/babel-preset-default"
		]
	},
	"devDependencies": {
		"@babel/cli": "^7.10.5",
		"@babel/core": "^7.11.1",
		"@wordpress/babel-preset-default": "^4.17.0",
		"@wordpress/eslint-plugin": "^7.1.0",
		"esm": "^3.2.25",
		"prettier": "^2.0.5",
		"rimraf": "^3.0.2"
	}
}